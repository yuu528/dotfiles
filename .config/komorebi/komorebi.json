{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.34/schema.json",
  "app_specific_configuration_path": "$Env:USERPROFILE/.config/komorebi/applications.json",
  "window_hiding_behaviour": "Cloak",
  "cross_monitor_move_behaviour": "Insert",
  "mouse_follows_focus": false,
  "default_workspace_padding": 1,
  "default_container_padding": 5,
  "resize_delta": 200,
  "border_padding": 0,
  "border_offset": -1,
  "border_width": 4,
  "border": true,
  "stackbar": {
    "height": 18,
    "mode": "OnStack",
    "tabs": {
      "width": 180,
      "font_size": 18,
      "fomt_family": "IBM Plex Sans JP"
    }
  },
  "floating_applications": [
    {
      "kind": "Exe",
      "id": "Zebar.exe",
      "matching_strategy": "Equals"
    },
    {
      "kind": "Exe",
      "id": "winlister.exe",
      "matching_strategy": "Equals"
    },
    {
      "kind": "Class",
      "id": "UnityWndClass",
      "matching_strategy": "Equals"
    },
    [
      {
        "kind": "Exe",
        "id": "LTspice.exe",
        "matching_strategy": "Equals"
      },
      {
        "kind": "Title",
        "id": "LTspice",
        "matching_strategy": "DoesNotStartWith"
      }
    ],
    [
      {
        "kind": "Exe",
        "id": "quartus.exe",
        "matching_strategy": "Equals"
      },
      {
        "kind": "Title",
        "id": "Quartus Prime Lite Edition",
        "matching_strategy": "DoesNotStartWith"
      }
    ],
    [
      {
        "kind": "Exe",
        "id": "GP6.exe",
        "matching_strategy": "Equals"
      },
      {
        "kind": "Title",
        "id": "GreenPAK Designer v",
        "matching_strategy": "DoesNotContain"
      }
    ],
    [
      {
        "kind": "Path",
        "id": "[^\\\\]*\\\\Gowin_V[^\\\\]*\\\\.*$",
        "matching_strategy": "Regex"
      },
      {
        "kind": "Title",
        "id": "GOWIN FPGA Designer - [",
        "matching_strategy": "DoesNotStartWith"
      }
    ],
    [
      {
        "kind": "Exe",
        "id": "gimp-[0-9].[0-9]*.exe",
        "matching_strategy": "Regex"
      },
      {
        "kind": "Title",
        "id": "â€“ GIMP",
        "matching_strategy": "DoesNotEndWith"
      },
      {
        "kind": "Title",
        "id": "GIMP (GNU Image Manipulation Program)",
        "matching_strategy": "DoesNotEqual"
      }
    ]
  ],
  "ignore_rules": [
    {
      "kind": "Class",
      "id": "OneNote::CNavigationUIPerMonitorDpiAwareWindow",
      "matching_strategy": "Equals"
    },
    {
      "kind": "Exe",
      "id": "AdskAccessUIHost.exe",
      "matching_strategy": "Equals"
    }
  ],
  "animation": {
    "enabled": true,
    "duration": 200,
    "style": "EaseInOutCirc"
  },
  "theme": {
    "name": "GruvboxDarkMedium",
    "palette": "Base16"
  }
}
