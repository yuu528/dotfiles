{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.31/schema.json",
  "app_specific_configuration_path": "$Env:USERPROFILE/.config/komorebi/applications.json",
  "window_hiding_behaviour": "Cloak",
  "cross_monitor_move_behaviour": "Insert",
  "mouse_follows_focus": false,
  "default_workspace_padding": 1,
  "default_container_padding": 5,
  "resize_delta": 200,
  "border_padding": 0,
  "border_offset": -1,
  "border_width": 4,
  "border": true,
  "monitors": [
    {
      "workspaces": [
        {
          "name": "1",
          "layout": "BSP"
        },
        {
          "name": "2",
          "layout": "BSP"
        },
        {
          "name": "3",
          "layout": "BSP"
        },
        {
          "name": "4",
          "layout": "BSP"
        },
        {
          "name": "5",
          "layout": "BSP"
        },
        {
          "name": "6",
          "layout": "BSP"
        },
        {
          "name": "7",
          "layout": "BSP"
        },
        {
          "name": "8",
          "layout": "BSP"
        },
        {
          "name": "9",
          "layout": "BSP"
        }
      ]
    }
  ],
  "stackbar": {
    "height": 18,
    "mode": "OnStack",
    "tabs": {
      "width": 180,
      "font_size": 18,
      "fomt_family": "IBM Plex Sans JP"
    }
  },
  "floating_applications": [
    {
      "kind": "Exe",
      "id": "Zebar.exe",
      "matching_strategy": "Equals"
    },
    {
      "kind": "Exe",
      "id": "quartus.exe",
      "matching_strategy": "Equals"
    },
    {
      "kind": "Exe",
      "id": "winlister.exe",
      "matching_strategy": "Equals"
    },
    [
      {
        "kind": "Exe",
        "id": "LTspice.exe",
        "matching_strategy": "Equals"
      },
      {
        "kind": "Title",
        "id": "LTspice",
        "matching_strategy": "DoesNotStartWith"
      }
    ]
  ],
  "ignore_rules": [
    {
      "kind": "Class",
      "id": "OneNote::CNavigationUIPerMonitorDpiAwareWindow",
      "matching_strategy": "Equals"
    },
    {
      "kind": "Exe",
      "id": "AdskAccessUIHost.exe",
      "matching_strategy": "Equals"
    }
  ],
  "animation": {
    "enabled": true,
    "duration": 200,
    "style": "EaseInOutCirc"
  },
  "theme": {
    "name": "GruvboxDarkMedium",
    "palette": "Base16"
  }
}
